import java.util.Scanner;
class evenodd{
     public static void main(String args[])
     {
        Scanner SK = new Scanner(System.in);
        int a = SK.nextInt();
        if(a%2==0)
        {
            System.out.println("Even");
        }
        else
        {
            System.out.println("Odd");
        }
     }
}  

